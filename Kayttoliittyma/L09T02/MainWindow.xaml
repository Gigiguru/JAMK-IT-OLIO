<Window x:Class="L09T2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:L09T2"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBlock x:Name="txtOutput" HorizontalAlignment="Left" Margin="58,201,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBox x:Name="txtInput" HorizontalAlignment="Left" Height="23" Margin="58,87,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" TextChanged="txtInput_TextChanged"/>
        <CheckBox x:Name="chMarkka" Content="Markkaa" HorizontalAlignment="Left" Margin="58,115,0,0" VerticalAlignment="Top" Checked="chMarkka_Checked"/>
        <CheckBox x:Name="chEuro" Content="Euroa" HorizontalAlignment="Left" Margin="58,135,0,0" VerticalAlignment="Top" Checked="chEuro_Checked"/>
        <Button x:Name="Laske" Content="Laske" HorizontalAlignment="Left" Margin="58,164,0,0" VerticalAlignment="Top" Width="75" Click="Laske_Click"/>

    </Grid>
</Window>
