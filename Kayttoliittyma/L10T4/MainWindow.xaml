        <Button x:Name="button1" Content="2" HorizontalAlignment="Left" Margin="312,94,0,0" VerticalAlignment="Top" Width="75" Click="Custom" />
        <Button x:Name="button2" Content="3" HorizontalAlignment="Left" Margin="312,119,0,0" VerticalAlignment="Top" Width="75"/>
