        <RadioButton x:Name="rdTemp" Content="Temp" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="312,222,0,0"/>
        <RadioButton x:Name="rdHum" Content="Hum
" HorizontalAlignment="Left" Margin="432,222,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button6" Content="0" HorizontalAlignment="Left" Margin="312,45,0,0" VerticalAlignment="Top" Width="75" Click="Custom"/>
        <Button x:Name="button7" Content="," HorizontalAlignment="Left" Margin="421,169,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.24,3.75" Click="Custom" />

    </Grid>
